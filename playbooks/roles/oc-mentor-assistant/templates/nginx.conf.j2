server {
    listen 80;
    server_name {{ server_domain }};

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root {{ static_root }};
    }

    location / {
        include proxy_params;
        proxy_read_timeout 300s;
        proxy_connect_timeout 75s;
        proxy_pass http://unix:/run/{{ app_name }}.sock;
    }
}